{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": "standard",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/jsx-uses-vars": 2,
        "react/prop-types": 0,
        "import/prefer-default-export": 0
    },
    "plugins": [
        "react"
    ]
}